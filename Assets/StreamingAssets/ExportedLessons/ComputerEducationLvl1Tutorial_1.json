            "Images": [],
            "InteractableImages": [],
            "IsBeforeTask": false,
            "TaskPrefabName": "",
            "HideCharacter": false,
            "CharacterIconPath": "UserImages/KiberBear Hello.png",
            "CharacterPosition": {
